<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:titlebar="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
     <com.pangff.wjw.view.TitleBar
	    android:layout_width="match_parent"
	    android:layout_height="@dimen/titleBarHeight"
	    titlebar:canBack="true"
	    titlebar:rightText="@string/withdrawals_explain"
	    titlebar:title="@string/withdrawals_apply" />
    
     
     
	<LinearLayout
	    android:orientation="horizontal"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_weight="1">
	 
	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="账户名称"/>
	    
	    <EditText
	        android:id="@+id/accountNameE"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"/>
	    
	 </LinearLayout>
	 
	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_weight="1">
	 
	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="$钱包余额"/>
	    
	    <TextView
	        android:id="@+id/accountRemainE"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:gravity="right"
	        android:text="1"/>
	    
	 </LinearLayout>
    
	 <LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_weight="1">
	 
	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="开户银行"/>
	    
	    <EditText
	        android:id="@+id/OaccountBankE"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"/>
	    
	  </LinearLayout>
	 
	  <LinearLayout
	     android:layout_width="match_parent"
	     android:layout_height="wrap_content"
	     android:layout_weight="1">
	 

	      
	     <TextView
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:text="支行名称"/>
	    
	     <EditText
	         android:id="@+id/branchNameE"
	         android:layout_width="match_parent"
	         android:layout_height="wrap_content"/>
	    
	 </LinearLayout>
	 
	 <LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_weight="1">
	 
	    <ImageView
	        android:id="@+id/cardI"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:background="#FFFFFF00"/>
	      
	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="银行帐号"/>
	    
	    <EditText
	        android:id="@+id/bankAccountE"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"/>
	    
	 </LinearLayout>
	  
	  <LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_weight="1">
	 
	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="$提现金额"/>
	    
	    <EditText
	        android:id="@+id/withdrawalCashE"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"/>
	    
	 </LinearLayout>
	  
	 <LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_weight="1">
	 
	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="支付密码"/>
	    
	    <EditText
	        android:id="@+id/payPasswordE"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"/>
	    
	 </LinearLayout>
	 
	 <LinearLayout
	     android:layout_width="match_parent"
	     android:layout_height="wrap_content"
	     android:layout_weight="1" >


	     <Button
	         android:id="@+id/WithdrawalB"
	         android:layout_width="match_parent"
	         android:layout_height="wrap_content"
	         android:text="提现" />
	 </LinearLayout>

</LinearLayout>